body {
	background-color: white;
}

button {
	background-color: #6AA9D0;
	border: .2rem white solid;
	color: white;
	min-width: 140px;
	height: 50px;
	font-size: 1.1rem;
	margin-right: 1.5rem;
	border-radius: .6rem;
}

.mainHolder {
	max-width: 1200px;
	margin: 0 auto;
}

.arrow_box {
	position: relative;
	background: rgba(255,255,255,0);
}
.arrow_box:after, .arrow_box:before {
	top: 100%;
	left: 50%;
	border: solid transparent;
	content: " ";
	height: 0;
	width: 0;
	position: absolute;
	pointer-events: none;
}

.arrow_box:after {
	border-color: rgba(102, 3, 3, 0);
	border-top-color: rgba(255,255,255,1);
	border-width: 20px;
	margin-left: -20px;
}
.arrow_box:before {
	border-color: rgba(102, 102, 102, 0);
	border-top-color: #666666;
	border-width: 23px;
	margin-left: -23px;
}

.headerNav {
	width: 100%;
	padding: 1.5rem 2.5% 0.8rem 2.5%;
	background-color: rgba(255,255,255, .5);
	color: #555;
	z-index: 1000;
	margin-bottom: 0;
	.navLogo {
		width: 20%;
		float: left;
	}
	.navLinks {
		width: 50%;
		float: left;
		ul li {
			padding: 0 1.5rem;
			font-size: .9rem;
			text-transform: uppercase;
			&.first {
				padding-left: 0;
			}
			a {
				color: #555;
			}
		}
	}
	.cart-link {
		width: 30%;
		float: left;
		button {
			float: right;
			font-size: .9rem;
			height: 40px;
			position: relative;
			bottom: 7px;
		}
		a {
			float: right;
			color: #555;
		}
	}
}

// HOMEPAGE STYLE ************************************ 

.homeHero {
	// background-image: url( {{ 'homeHeroBackgroundImage.jpg' }} );
	background-size: cover;
	background-repeat: no-repeat;
	padding: 5rem 2.5% 0 2.5%;
	background-position: top center;
	text-align: center;
	color: #222;
	margin-bottom: 0;
	background: -webkit-radial-gradient(circle, #fff, #CACACA);
	/* Safari 4-5, Chrome 1-9 */
  	background: -webkit-gradient(radial, center center, 0, center center, 460, from(#fff), to(#CACACA));
	/* Safari 5.1+, Chrome 10+ */
	background: -webkit-radial-gradient(circle, #fff, #CACACA);
	/* Firefox 3.6+ */
	background: -moz-radial-gradient(circle, #fff, #CACACA);
	/* IE 10 */
	background: -ms-radial-gradient(circle, #fff, #CACACA);
	h1 {
		font-size: 4rem;
		color: #222;
		font-weight: 400;
		span {
			font-size: 6.25rem;
			font-weight: 700;
		}
	}
	img {
		top: 6px;
		position: relative;
		padding-top: 2rem;
	}
	
}

.process {
	padding: 7rem 0;
	text-align: center;
	h1 {
		font-size: 3.8rem;
		color: #222;
		font-weight: 700;
	}
	h2 {
		font-weight: 400;
		font-size: 1.2rem;
	}
	p {
		font-size: 1rem;
	}
	.steps {
		display: -webkit-box;
		display: -moz-box;
		display: -ms-flexbox;
		display: flex;
		flex-flow: wrap;
		padding-top: 4rem;
		h4{
			font-size:1.5rem;
			padding-top:1rem;
		}
		.step {
			-webkit-box-flex: 2 190px;
			-moz-box-flex:  2 190px;
			flex:  2 190px;
			margin: 0 auto;
			text-align: center;
		}
		.progress {
			-webkit-box-flex: 1 95px;
			-moz-box-flex: 1 95px;
			flex: 1 95px;
			margin: 0 auto;
			opacity:.5;
		}
	}
}

.contentRight {
	background: -webkit-linear-gradient(left, #9DCEEC, #6AA9D0); /* For Safari 5.1 to 6.0 */
	background: -o-linear-gradient(left, #9DCEEC, #6AA9D0); /* For Opera 11.1 to 12.0 */
	background: -moz-linear-gradient(left, #9DCEEC, #6AA9D0); /* For Firefox 3.6 to 15 */
	background: linear-gradient(left, #9DCEEC, #6AA9D0); /* Standard syntax */
	min-height: 520px;
	color: white;
	margin-bottom: 0;
	padding-bottom: 70px;
	.contentRightHolder {
		padding-top: 70px;
